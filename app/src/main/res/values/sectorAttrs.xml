<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SectorView">
        //兼容用系统属性
        <attr name="android:layout_width" format="dimension"/>
        <attr name="android:layout_height" format="dimension"/>
        //自定义属性
        <attr name="sectorRadius" format="dimension" />
        <attr name="sectorStroke" format="dimension"/>
        <attr name="sectorStartAngle" format="float"/>
        <attr name="sectorSweepAngle" format="float"/>
        <attr name="sectorGradientStart" format="color"/>
        <attr name="sectorGradientEnd" format="color"/>
    </declare-styleable>
</resources>
